<apex:page standardController="Account" recordSetVar="accounts" sidebar="true">
     <apex:pageBlock >
        <apex:pageBlockTable value="{!accounts}" var="a">
            <apex:column value="{!a.name}"/>        
            <apex:column value="{!a.Phone}"/>        
        </apex:pageBlockTable>     
     </apex:pageBlock>
</apex:page>